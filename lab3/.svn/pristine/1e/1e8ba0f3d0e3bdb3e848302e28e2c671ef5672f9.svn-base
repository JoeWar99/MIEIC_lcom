int sys_inb_counter=0;

#ifdef LAB3
int sys_inb_cnt(port_t port, uint32_t*byte);
#else
#define sys_inb_cnt(p,q) sys_inb_cnt(p,q)
#endif

int sys_inb_cnt(port_t port, uint32_t*byte){
  sys_inb_cnt(port,byte);
  sys_inb_counter++;
  return 0;
}

